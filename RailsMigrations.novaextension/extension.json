{
    "identifier": "wylan.RailsMigrations",
    "name": "Rails Migrations",
    "vendor": "Wylan",
    "description": "Provides commands to easily access migrations in your Rails projects.",
    "version": "1.0.1",
    "repository": "https://github.com/Wylan/nova-rails-migrations",
    "bugs": "https://github.com/Wylan/nova-rails-migrations/issues",
    "categories": [
        "commands"
    ],
    "main": "main.js",
    "entitlements": {
        "filesystem": "readwrite"
    },
    "commands": {
        "command-palette": [
		    {
		    	"title": "Open Latest Migration",
		    	"command": "rails-migrations.openLatestMigration"
		    },
		    {
		    	"title": "Choose Migration",
		    	"command": "rails-migrations.listMigrations"
		    }
	    ]
    }
}
